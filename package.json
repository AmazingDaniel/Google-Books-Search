  },
  "dependencies": {
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "mongoose": "^5.11.0",
    "path": "^0.12.7"
  },
  "homepage": "https://github.com/AmazingDaniel/Google-Books-Search#readme",
